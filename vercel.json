{
  "buildCommand": "cd packages/frontend && pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": null,
  "outputDirectory": "packages/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/dashboard.js": {
      "maxDuration": 30
    }
  }
}