    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}