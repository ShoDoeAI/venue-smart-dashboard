{
  "buildCommand": "cd packages/frontend && pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": null,
  "outputDirectory": "packages/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/dashboard.ts": {
      "maxDuration": 30
    },
    "api/chat.ts": {
      "maxDuration": 30
    },
    "api/cron/fetch-data.ts": {
      "maxDuration": 300
    }
  },
  "env": {
  }
}