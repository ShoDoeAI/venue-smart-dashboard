{
  "buildCommand": "cd packages/frontend && pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": null,
  "outputDirectory": "packages/frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/frontend/dist/$1"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  }
}