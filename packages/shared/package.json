{
  "name": "@venuesync/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared types, schemas, and utilities for VenueSync",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "rm -rf dist",
