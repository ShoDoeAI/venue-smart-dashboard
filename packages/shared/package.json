    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.5",
    "axios": "^1.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}